{
  "schemaVersion": 1,
  "id": "human_reborn",
  "version": "${version}",
  "name": "Human Reborn",
  "description": "This mod adds a modern version of the Human mob from Minecraft Beta 1.7.3!",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/human-reborn/issues/",
    "sources": "https://github.com/SmushyTaco/Human-Reborn/"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "icon": "assets/human_reborn/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.human_reborn.HumanReborn"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.human_reborn.client.HumanRebornClient"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}"
  }
}